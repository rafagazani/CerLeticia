<%= form_for(@post, :html => {:multipart => true}) do |f| %>
  <% if @post.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@post.errors.count, "error") %> Formulário com erro:</h2>

      <ul>
      <% @post.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>




     
 
   <form class="col s6">

      <div class="row"> 
        <div class="input-field col s6">
          <%= f.label :paciente_id %><br><br>
          <%= f.text_field :paciente_id %>
        </div>
      </div>

      <div class="row"> 
        <div class="input-field col s6">
          <%= f.label :titulo %><br><br>
          <%= f.text_field :titulo %>
        </div>
      </div>

      <div class="row"> 
        <div class="input-field col s6">
          <%= f.label :Imagem %><br><br>
          <%= f.file_field :image %>
        </div>
      </div>

      <div class="row"> 
        <div class="input-field col s6">
          <%= f.label :Áudio %><br><br>
          <%= f.file_field :audio %>
        </div>
      </div>

       <p>       
      </p>

  <button class="actions btn waves-effect waves-light">
    <%= f.submit %>
  </button>

      <div class="actions">
       
      </div>
    </form>
<% end %>
